{
  "name": "casequest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://www.casequestapp.com",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "clean": "rm -rf dist node_modules && npm install",
    "rebuild": "npm run clean && npm run build",
    "redeploy": "npm run rebuild && npm run deploy",
    "restore-domain": "git checkout gh-pages && echo 'www.casequestapp.com' > CNAME && git add CNAME && git commit -m 'restore domain' || echo 'CNAME already ok' && git push && git checkout -",
    "test:ui": "playwright test --reporter=line",
    "test:perf": "lhci autorun"
  },
  "dependencies": {
    "@sentry/react": "^10.19.0",
    "@sentry/vite-plugin": "^4.3.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.545.0",
    "posthog-js": "^1.274.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.0",
    "@types/node": "^24.7.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14",
    "vite": "^7.1.7"
  }
}
